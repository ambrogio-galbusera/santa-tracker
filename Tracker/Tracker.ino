void sensorISR()
{
  sensorDetected = true;
  
  unsigned long currMicros = micros();
  if (prevMicros != 0)
  {
    rotDuration = currMicros - prevMicros;
    if (rotDuration < minDuration)
      minDuration = rotDuration;
    if (rotDuration > maxDuration)
      maxDuration = rotDuration;
  }

  detects ++;
  prevMicros = currMicros;
}

void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  Serial.println("Start");

  pinMode(SENSOR_PIN, INPUT_PULLUP);
  attachInterrupt(digitalPinToInterrupt(SENSOR_PIN), sensorISR, RISING);

  FastLED.addLeds<WS2812, LEDS_PIN, GRB>(leds, TOTAL_LEDS);
  rainbow();

  setupDisplay();
  updateDisplay();
  
  setupWifi();

  appStatus = APP_STARTUP;
  santaX = santaY = -1;
  startMillis = millis();
}

void loop() {
  if (appStatus == APP_MAP)
  {
    if (sensorDetected)
    {
      updateLEDs(GLOBE_BW, true);
      sensorDetected = false;
    }

    unsigned long delta = millis() - mapMillis;
    if (delta > MAP_TIME_MS)
    {
      appStatus = APP_SNOWFLAKE;
      updateDisplay();

      snowFlakeMillis = millis();
    }

    delta = millis() - fetchMillis;
    if (delta > FETCH_TIME_MS)
    {
      rainbow();
      requestPosition();

      appStatus = APP_FETCHING;
      updateDisplay();

      fetchMillis = millis();
      updateMillis = millis();
    }
  }
  if (appStatus == APP_SNOWFLAKE)
  {
    if (sensorDetected)
    {
      updateLEDs(SNOWFLAKE, false);
      sensorDetected = false;
    }

    unsigned long delta = millis() - snowFlakeMillis;
    if (delta > SNOWFLAKE_TIME_MS)
    {
      appStatus = APP_MAP;
      updateDisplay();

      mapMillis = millis();
    }
  }
  else if (appStatus == APP_FETCHING)
  {
    if (sensorDetected)
    {
      rainbow();
      sensorDetected = false;
    }

    unsigned long delta = millis() - updateMillis;
    if (delta > 3000)
    {
      if (checkResponse())
      {
        appStatus = APP_MAP;
        updateDisplay();

        mapMillis = millis();
      }
    }
  }
  else if (appStatus == APP_STARTUP)
  {
      rainbow();
      requestPosition();
      appStatus = APP_FETCHING;
      updateDisplay();

      fetchMillis = millis();
      updateMillis = millis();
  }
}
